#include <bits/stdc++.h>
#include <cstdlib>
using namespace std;
#define int long long
#define piii pair<int,int>
//sort(puch,puch+n,greater<int>());
struct place{
	int x,y,step;
};
const int N=2e4;
queue<place>q;
char mp[N];
bool vis[N];
int n,m,sx,sy,ex,ey;
int dx[8]={1,1,-1,1,0,-1,0,-1}, dy[8]={0,1,0,-1,1,1,-1,-1};
bool check(int x,int y){
	if(x<1||x>n||y<1||y>m||mp[(x-1)*m+y]=='X'){
		return 0;
	}
	else return 1;
}
bool look(int stx,int sty){
	for(int i=0;i<8;i++){
		int xx=dx[i]+stx;
		int yy=dy[i]+sty;
		while(check(xx,yy)){
			if(xx==ex&&yy==ey){
				return 1;
			}
			xx+=dx[i];
			yy+=dy[i];
		}
	}
	return 0;
}
signed main(){
	cin>>n>>m;
	for(int i=1;i<=n;i++){
		for(int j=1;j<=m;j++){
			cin>>mp[(i-1)*m+j];
		}
	}
	while(1){
		cin>>ex>>ey>>sx>>sy;
		if(sx==sy&&sy==ex&&ey==ex&&ex==0){
			return 0;
		}
		while(q.size()){
			q.pop();
		}
		memset(vis,0,sizeof(vis));
		q.push({sx,sy,0});
		vis[(sx-1)*m+sy]=1;
		bool flag=1;
		while(q.size()){
			place now=q.front();
			q.pop();
			if(now.x==ex&&now.y==ey||look(now.x,now.y)){
                cout<<now.step<<'\n'; 
				flag=0;
				break;
			}
			for(int i=0;i<8;i+=2){
				int xx=now.x+dx[i];
				int yy=now.y+dy[i];
				if(!check(xx,yy)||vis[(xx-1)*m+yy]){
					continue;
				}
				q.push({xx,yy,now.step+1});
				vis[(xx-1)*m+yy]=1;
			}
		}
		if(flag){
			cout<<"Poor Harry\n";
		}
	}
	return 0;
}        
